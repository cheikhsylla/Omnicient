{
  "name": "back-end-kit",
  "version": "1.0.0",
  "description": "back end test exercise(s) designed to test new candidates",
  "main": "src/main.js",
  "author": "thibaut.courtois@myomniscient.com",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "setup": "npm install -g yarn && yarn",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll --detectOpenHandles",
    "lint": "npx eslint . --ext .js --fix",
    "scratchpad": "npx babel-node ./src/scratchpad.js --trace-warnings",
    "start": "npx babel-node ./src/main.js --trace-warnings",
    "bundle": "git archive --format zip --output bundle-cli.zip master"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.15.4",
    "babel-jest": "^27.2.3",
    "core-js": "^3.18.1",
    "cowsay": "^1.5.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.2",
    "lodash": "^4.17.21"
  }
}
